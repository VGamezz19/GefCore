      <form method="post">
        <div class ="col-ms-12 col-md-12 col-xs-12 col-lg-12">
          <label>Email:</label>
          <input tabindex="1" type="text" name="email" autofocus/>
        </div>
        <div class ="col-ms-6 col-md-6 col-xs-12 col-lg-6" style="margin-top:1em !important;">
          <input type="submit" tabindex="1" class ="btn" value="restaurar contraseña"></input>
        </div>
        <div class ="col-ms-4 col-md-4 col-xs-12 col-lg-4 col-ms-offset-1 col-md-offset-1 col-xs-offset-1 col-lg-offset-1"  style="margin-top:1em !important;">
